# Environment Variables
NODE_ENV=production

# Database
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=chikox
DATABASE_URL=postgresql://postgres:password@localhost:5432/chikox

# JWT
JWT_ACCESS_SECRET=your-access-secret
JWT_REFRESH_SECRET=your-refresh-secret
JWT_ACCESS_EXPIRES_IN=15m
JWT_REFRESH_EXPIRES_IN=7d

# Cookie Secret
COOKIE_SECRET=your-cookie-secret

# CORS
CORS_ORIGIN=https://chikox.net,https://admin.chikox.net

# Public URLs
NEXT_PUBLIC_API_URL=https://chikox.net/api/v1
VITE_API_URL=https://chikox.net/api/v1
